{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/cli": "^2.45.0",
    "@sentry/react": "^9.17.0",
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run sentry:sourcemaps",
